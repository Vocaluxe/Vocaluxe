  <Target Name="managedDepCopyAfterBuild" BeforeTargets="AfterBuild">
    <Message Text="move all libs to the managed dependencies folder" Importance="normal"/>
    <ItemGroup>
      <RootDLLFilesToMove Include="$(TargetDir)*.dll"/>
    </ItemGroup>
    <ItemGroup>
      <x64FolderFilesToMove Include="$(TargetDir)x64\*.*"/>
    </ItemGroup>	
	<ItemGroup>
      <x86FolderFilesToMove Include="$(TargetDir)x86\*.*"/>
    </ItemGroup>
	<ItemGroup>
      <OtherFilesToMove Include="$(TargetDir)System.Data.SQLite.xml;"/>
    </ItemGroup>
    <Copy
        SourceFiles="@(RootDLLFilesToMove)"
        DestinationFiles="@(RootDLLFilesToMove->'$(TargetDir)libs\managed\%(Filename)%(Extension)')"/>
	<Copy
        SourceFiles="@(x64FolderFilesToMove)"
        DestinationFiles="@(x64FolderFilesToMove->'$(TargetDir)libs\managed\x64\%(Filename)%(Extension)')"/>
	<Copy
        SourceFiles="@(x86FolderFilesToMove)"
        DestinationFiles="@(x86FolderFilesToMove->'$(TargetDir)libs\managed\x86\%(Filename)%(Extension)')"/>
	<Copy
        SourceFiles="@(OtherFilesToMove)"
        DestinationFiles="@(OtherFilesToMove->'$(TargetDir)libs\managed\%(Filename)%(Extension)')"/>
	<Delete
        Files="@(RootDLLFilesToMove)" />
	<Delete
        Files="@(x64FolderFilesToMove)" />
	<Delete
        Files="@(x86FolderFilesToMove)" />
	<Delete
        Files="@(OtherFilesToMove)" />
	<RemoveDir
        Directories="$(TargetDir)\x64\;$(TargetDir)\x86\" />
  </Target>
